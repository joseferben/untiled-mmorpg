    "@remix-run/serve": "^1.17.1",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^8.4.0",
    "dotenv": "^16.3.1",
    "easystarjs": "^0.4.4",
    "immer": "^10.0.2",
    "isbot": "^3.6.10",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^6.4.1",
    "tiny-invariant": "^1.3.1",
