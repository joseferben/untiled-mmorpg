    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "tiled-types": "^1.3.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
