{
  "name": "untitled-mmorpg",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "test": "vitest",
    "bench": "vitest bench",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck",
    "repl": "tsx bin/repl.ts"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^4.15.0",
